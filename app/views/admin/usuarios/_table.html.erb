<%- model_class=Usuario -%>





<div class="accordion" id="accordionExample">
  <div class="accordion-item">
    <h2 class="accordion-header">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#Usuario" aria-expanded="true" aria-controls="Usuario">
        <%= model_class.model_name.human(count: 2).titleize %>
      </button>
    </h2>
    <div id="Usuario" class="accordion-collapse collapse <%= show_collapse %>" data-bs-parent="#accordionExample">
      <div class="accordion-body">
        <%= render partial: "usuarios/datatable" , locals: {remote: remote} %>
      </div>
    </div>
  </div>
</div>