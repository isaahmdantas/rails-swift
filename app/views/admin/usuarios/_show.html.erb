<%- model_class = Usuario -%>

<% resource ||= resource %>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
        
          <dt><strong><%= model_class.human_attribute_name(:nome) %> :</strong></dt>
          <dd><%= resource.try(:nome) %></dd>
        
              
          <dt><strong><%= model_class.human_attribute_name(:permissao) %> :</strong></dt>
          <dd><%= resource.try(:permissao) %></dd>
        
              
          <dt><strong><%= model_class.human_attribute_name(:email) %> :</strong></dt>
          <dd><%= resource.try(:email) %></dd>
        
              
          <dt><strong><%= model_class.human_attribute_name(:password) %> :</strong></dt>
          <dd><%= resource.try(:password) %></dd>
        
      
    </dl>
  </div>
  <div class="col-md-12">
    <%= render partial: "admin/audits/table", locals: {audits: resource.try(:own_and_associated_audits), show_collapse: 'show'} %>
  </div>
</div>